hotline click
<script>
document.addEventListener("DOMContentLoaded", function(event) {
  jQuery('a[href*="tel:"]').on('click', function() {
       gtag('event', 'conversion', {'send_to': 'AW-951104150/11111111111111'}); // Ads conversion
       gtag('event', 'click', {'event_category' : 'click_hotline'}); // Analytics event tracking
       window.dataLayer.push({'event': 'clic_hotline'}); // User dataLayer with GTM
  });
}); 
</script>

