public class Main{
public static void main(String args[]){
int[] dayso = {5,6,9};
inradayso(dayso);
}

public static boolean isPrime(int conso){
for(int j = 2; j < conso; j++){
if(conso % j == 0){
return false;
}
} // for 1
return true;
}

public static void inradayso(int[] dayso){
for(int i = 0; i < dayso.length; i++){

if(isPrime(i)){
System.out.println("True");
}else{
System.out.println("False");
}

} // for 2
} // method
} // class
